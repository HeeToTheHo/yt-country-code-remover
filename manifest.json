{
    "name": "Youtube Country ID Remover",
    "version": "1.0",
    "description": "aka anti-dox-gooby extension",
    "manifest_version": 2,
    "permissions": [
        "declarativeContent"
    ],
    "content_scripts": [
        {
            "matches": ["*://youtube.com/*"],
            "js": ["/content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "images/icon32.png"
    }
  }